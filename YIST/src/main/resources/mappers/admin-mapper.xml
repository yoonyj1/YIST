<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"mybatis-3-mapper.dtd" >

<mapper namespace="adminMapper">
	
	<select id="selectListCount" resultType="_int">
	select
		count(*)
	from member
	where
		status = 'Y'
	and
		sort = 2
	</select>


</mapper>